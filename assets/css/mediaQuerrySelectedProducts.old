/* xs=0 */

/* start of sm */
@media (min-width: 0px) and (max-width:575px){
    /* start of global */
    .container > h2{
        font-size: 32px  !important;
        line-height: 48px;
      }
/* end of global */

/* start of bar announcement */
.sales{
    margin: auto;
}
.phone-number,.login{
    display: none;
}
/* End of bar announcement */

/* Start of selected products */
.cart-product-title{
    margin-left: -10px; 
 }
 .quantity{
    margin-left: 140px;
 }
 .input-group{
    width: 100%!important;
 }
 .title-btn-text-cart{
    width:clamp(100px,100%) !important;
}
.cart-price{
   margin-left: 7px;
}
.title-btn-text-cart h2{
    line-height: 10px;
    margin-bottom: 10px !important;
}
.title-btn-text-cart h2 ~ p{
    margin-bottom: 7px;
    margin-top: 5px;
}
/* End of selected products */

}
/* end of sm */

/* start of md */
@media (min-width: 576px) and (max-width:767px){
/* start of global */
.container > h2{
    font-size: 32px  !important;
    line-height: 48px;
  }
/* end of global */
    /* start of bar announcement */
    .sales{
        margin: auto;
    }
    .phone-number,.login{
        display: none;
    }
    /* End of bar announcement */

    /* Start of selected products */
    .cart-product-title{
        margin-left: -10px;
     }
     .quantity{
        margin-left: 170px;
     }


     /* Start of selected products */



}
/* end of md */

/* start of lg */
@media (min-width: 768px) and (max-width:991px){
    /* start of global */
    .container > h2{
        font-size: 40px  !important;
        line-height: 48px;
      }
    /* end of global */

/* start of bar announcement */
.sales{
    margin: auto;
}
.phone-number,.login{
    display: none;
}
/* End of bar announcement */



}
/* end of lg */

/* start of xl */
@media (min-width: 992px) and (max-width: 1199px){

/* Start of selected products */
.cart-product-title{
    margin-left: -10px; 
 }
 .quantity{
    margin-left: 140px;
 }
 .input-group{
    width: 100%!important;
 }
/* End of selected products */


/* start of footer */
footer .input-group{
    width: 80%!important;
 }
    
/* End of footer */

}
/* end of xl */

