/* font-family: Poppins, sans-serif ;*/
@import url('https://fonts.googleapis.com/css2?family=Poppins:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,900;1,300;1,400;1,500;1,600;1,700&family=Raleway:wght@100;300;400;500;700;800;900&display=swap');
:root{
    --MainColor:#00234D;
    --SecondaryColor: #F76B6A;
    
}
body{font-family: 'Poppins', sans-serif ;}
.bg-MainColor{background-color:var(--MainColor) !important;}
.MainColor{color:var(--MainColor) !important;}
.bg-SecondaryColor{background-color:var(--SecondaryColor) !important;}
.SecondaryColor{color:var(--SecondaryColor) !important;}
/* Main classes above! ^ */

/* Start of selected products */
.selected-products{
    margin-top: 100px;
}
.selected-products img{
    width:80px;
    height: 100px;
}

.selected-products input{
    width:50px;
    outline: none;
    font-size: 13px;
    line-height:19.5px;
    height:35px;
  }
.selected-products .input-group input::placeholder{
    font-size: 13px;
  }
.selected-products .input-group-text{
    padding-inline: 9px;
    padding-block: 4px;
    user-select: none;
    height:35px;
  }
.selected-products  .input-group-text:hover{
    cursor: pointer;
  }
  
.selected-products input{
    border:1px solid rgba(0,0,0,0.1);
    text-align: center;
  }
 .selected-products .remove-a{
    font-size: 11px;
    line-height: 11px;
    color: black;
    display: flex;
    justify-content: center;
    margin-top: 6px;
    transition: 0.3s;

  }
  .remove-a:hover{
    cursor: pointer;
    color:var(--SecondaryColor);
    text-decoration: none !important;
  }
  p{
    font-size: 13px;
    line-height: 19.5px;
  }
  .table-head h2{
    font-size: 15px;
    font-weight: 500;
    line-height: 19.5px;
    transition: 0.3s;
  }
  h2:hover{
    color: var(--SecondaryColor) !important;
    cursor: pointer;
  }
  .cart-item{
    padding:8px 8px;
  }
  input::-webkit-outer-spin-button,
  input::-webkit-inner-spin-button {
  -webkit-appearance: none;
  
}
.cart-product-title a{
    font-weight: 500;
}
.border-bottom{
    border-bottom:1px solid rgba(0,0,0,0.1);
}
.table-title{
    font-size:18px;
    font-weight:500;
    line-height:27px;
    
}
.cart-product-title{
   
   margin-left: -70px;
   width: 200px;
   height: 100px; 
}
.title-btn-text-cart h2 a,.cart-product-title h2{
    font-size: 15px;
    font-weight: 400;
    line-height: 10px;
    transition: 0.3s;
}

.title-btn-text-cart h2{
    line-height: 10px;
}
.title-btn-text-cart h2{
    display: inline;
}
/* Start of cart totals */
.cart-total-counts{
    background-color: #F6F6F9;
}
.cart-totals-title{
    font-size:18px;
    font-weight:500;
    line-height:27px;
    margin-bottom: 28px;
}

.cart-total-counts h4{
    font-weight: 400;
    font-size:16px;
    line-height:27px;
}
.cart-total-counts p{
    font-size:16px ;
    line-height:27px;
}
.cart-total-counts span{
    font-size:12px;
    line-height:20px;
}
.cart-total-counts button{
    font-size:14px;
    font-weight:500;
    line-height:14px;
    height: 40px;
    padding-inline: 17px;
    letter-spacing: 0.5px;
}
/* End of Cart totals */

 
/* End of selected products */