/* start of lg */
@media (min-width: 768px) and (max-width:992px){
    /* start of global */
    .container > h2{
        font-size: 40px  !important;
        line-height: 48px;
      }
    /* end of global */

    /* Start of checkout */
    .bg-done::before{
        left:-85%;
    }
    .bg-notDone::before{
        left:-280%;
        width:460%;
    }
    .bg-notDone.special::before{
        left: -250%;
    }


    /* End of checkout */
