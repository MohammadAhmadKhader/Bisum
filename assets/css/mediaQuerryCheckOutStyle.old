/* xs=0 */

/* start of sm */
@media (min-width: 0px) and (max-width:575px){
/* start of global */
.container > h2{
    font-size: 32px  !important;
    line-height: 48px;
}
/* end of global */

/* start of bar announcement */
.sales{
    margin: auto;
}
.phone-number,.login{
    display: none;
}
/* End of bar announcement */
/* Start of chechout */
.process-steps-list {
    width: auto !important;
}
.responsive-lists > ul{
    display:inline-block !important;
    margin-top:10px;
    margin-right:15px;
}
.responsive-lists > ul >li{
    margin-bottom: 30px;
}
.responsive-lists .process-steps-list li{
    margin-top: 13px;
    margin-bottom: 56px;
}
.bg-done::before{
    content:none;
}
.bg-notDone::before{
    content:none;
}
.user-profile{
    margin-top: 15px;
    flex-direction: column;
    justify-content: center;
    row-gap: 20px;
}
.user-profile .user-profile-edit-btn{
    text-align: center;
    margin-left: 0px !important;
}
.user-profile .user-info{
    text-align: center;
}
/* End of checkout */




}
/* end of sm */

